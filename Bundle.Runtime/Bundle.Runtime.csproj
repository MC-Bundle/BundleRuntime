<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<LangVersion>10.0</LangVersion>
	<ImplicitUsings>false</ImplicitUsings>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<PackageVersion>0.2.2-alpha</PackageVersion>
	<VersionPrefix>0.2.2-alpha</VersionPrefix>
	<Authors>D5</Authors>
	<Company>Bundle</Company>
	<Description>Core library for Bundle Client Engine and Bundle Server Engine</Description>
	<Copyright>MIT</Copyright>
	<PackageProjectUrl>https://github.com/MC-Bundle</PackageProjectUrl>
	<PackageIcon>logo.png</PackageIcon>
	<RepositoryUrl>https://github.com/MC-Bundle/BundleRuntime.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageTags>bundle;minecraft;майнкрафт;</PackageTags>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
	<ItemGroup>
		<None Include="logo.png" Pack="true" PackagePath="/"/>
	</ItemGroup>
</Project>
